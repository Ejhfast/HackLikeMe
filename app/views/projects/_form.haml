- form_for :project do |f|
	= f.label :name
	%br
	= f.text_field :name
	%br
	= f.label :Website
	%br
	= f.text_field :url
	%br
	= f.label :description
	%br
	= f.text_area :description
	= f.hidden_field :user_id, :value => current_user.id || -1
	%br
	= f.label :looking_for
	%br
	= select :project, :category_id, Category.all.map{|x| [x.name, x.id]}
	= f.submit